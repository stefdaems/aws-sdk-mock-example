{
  "name": "aws-sdk-mock-example",
  "version": "0.1.0",
  "bin": {
    "aws-sdk-mock-example": "bin/aws-sdk-mock-example.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "aws-cdk": "2.76.0",
    "aws-sdk-mock": "^5.8.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.76.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1414.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "jest": {
    "setupFiles": [
      "./test/jestSetup.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/(test|src)/**/*(*.)@(spec|test).ts?(x)",
      "<rootDir>/**/*(*.)@(spec|test).ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "preset": "ts-jest"
  }
}
